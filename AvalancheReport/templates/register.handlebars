<div class="col-md-4 col-md-offset-4 loginDiv2">
    <form action="" id="register-form">
        <h1 class="login">SIGN UP</h1>
        <div class="form-group">
            <label for="rg-username labelName">Username: </label>
            <input id="rg-username" class="form-control" placeholder="Username" required>
        </div>

        <div class="form-group">
            <label for="rg-password labelName">Password: </label>
            <input id="rg-password" type="password" class="form-control" placeholder="Password" required>
        </div>

        <div class="form-group">
            <label for="rg-email labelName">Email: </label>
            <input id="rg-email" type="email" class="form-control" placeholder="Email" required>
        </div>
        <div class="form-group">
            <label for="rg-address labelName">Address: </label>
            <input id="rg-address" class="form-control" placeholder="Address" required>
        </div>
        <div class="form-group">
            <label for="rg-phone labelName">Phone №: </label>
            <input type="number" id="tb-phone" class="form-control" placeholder="Phone №" required>
        </div>
        <a href="#/home" type="submit" class="btn btn-warning logBTN" id="btn-register">SIGN UP </a>
    </form>
</div>